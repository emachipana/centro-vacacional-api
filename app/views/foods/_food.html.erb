<div id="<%= dom_id food %>">
  <p>
    <strong>Name:</strong>
    <%= food.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= food.price %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= food.amount %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= food.description %>
  </p>

  <p>
    <strong>Primer:</strong>
    <%= food.primer %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= food.type %>
  </p>

  <p>
    <strong>State:</strong>
    <%= food.state %>
  </p>

</div>
