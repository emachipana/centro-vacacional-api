<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= user.token %>
  </p>

  <p>
    <strong>Client:</strong>
    <%= user.client_id %>
  </p>

</div>
